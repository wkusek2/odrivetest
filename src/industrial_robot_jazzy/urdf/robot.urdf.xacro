<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="industrial_robot_6dof">

  <xacro:property name="PI" value="3.141592653589793"/>

  <!-- MateriaÅ‚y -->
  <material name="orange"><color rgba="1.0 0.5 0.0 1"/></material>
  <material name="dark_grey"><color rgba="0.3 0.3 0.3 1"/></material>
  <material name="light_grey"><color rgba="0.7 0.7 0.7 1"/></material>
  <material name="blue"><color rgba="0.0 0.0 0.8 1"/></material>

  <!-- BASE -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="dark_grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="20"/>
      <origin xyz="0 0 0.05"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.5"/>
    </inertial>
  </link>

  <!-- LINK1 -->
  <link name="link1">
    <visual>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.08" length="0.3"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.08" length="0.3"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="8"/>
      <origin xyz="0 0 0.15"/>
      <inertia ixx="0.5" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>

  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="$(eval -${PI})" upper="$(eval ${PI})" effort="200" velocity="2.0"/>
  </joint>

  <!-- LINK2 -->
  <link name="link2">
    <visual>
      <origin xyz="0.25 0 0" rpy="0 $(eval ${PI} / 2) 0"/>
      <geometry>
        <cylinder radius="0.06" length="0.5"/>
      </geometry>
      <material name="light_grey"/>
    </visual>
    <collision>
      <origin xyz="0.25 0 0" rpy="0 $(eval ${PI} / 2) 0"/>
      <geometry>
        <cylinder radius="0.06" length="0.5"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="6"/>
      <origin xyz="0.25 0 0"/>
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.6" iyz="0.0" izz="0.6"/>
    </inertial>
  </link>

  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0.3" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="$(eval -${PI} / 2)" upper="$(eval ${PI} / 2)" effort="150" velocity="2.0"/>
  </joint>

  <!-- LINK3 -->
  <link name="link3">
    <visual>
      <origin xyz="0.2 0 0" rpy="0 $(eval ${PI} / 2) 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.4"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin xyz="0.2 0 0" rpy="0 $(eval ${PI} / 2) 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.4"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="4"/>
      <origin xyz="0.2 0 0"/>
      <inertia ixx="0.05" ixy="0.0" ixz="0.0" iyy="0.3" iyz="0.0" izz="0.3"/>
    </inertial>
  </link>

  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0.5 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="$(eval -${PI})" upper="$(eval ${PI})" effort="100" velocity="3.0"/>
  </joint>

  <!-- LINK4 -->
  <link name="link4">
    <visual>
      <origin xyz="0 0 0.08" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.04" length="0.16"/>
      </geometry>
      <material name="dark_grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.08" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.04" length="0.16"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2"/>
      <origin xyz="0 0 0.08"/>
      <inertia ixx="0.02" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.01"/>
    </inertial>
  </link>

  <joint name="joint4" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0.4 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="$(eval -${PI})" upper="$(eval ${PI})" effort="50" velocity="4.0"/>
  </joint>

  <!-- LINK5 -->
  <link name="link5">
    <visual>
      <origin xyz="0.06 0 0" rpy="0 $(eval ${PI} / 2) 0"/>
      <geometry>
        <cylinder radius="0.035" length="0.12"/>
      </geometry>
      <material name="light_grey"/>
    </visual>
    <collision>
      <origin xyz="0.06 0 0" rpy="0 $(eval ${PI} / 2) 0"/>
      <geometry>
        <cylinder radius="0.035" length="0.12"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.5"/>
      <origin xyz="0.06 0 0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>

  <joint name="joint5" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0 0 0.16" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="$(eval -${PI} / 2)" upper="$(eval ${PI} / 2)" effort="50" velocity="4.0"/>
  </joint>

  <!-- LINK6 -->
  <link name="link6">
    <visual>
      <origin xyz="0 0 0.03" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.03" length="0.06"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.03" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.03" length="0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <origin xyz="0 0 0.03"/>
      <inertia ixx="0.005" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.002"/>
    </inertial>
  </link>

  <joint name="joint6" type="revolute">
    <parent link="link5"/>
    <child link="link6"/>
    <origin xyz="0.12 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="$(eval -${PI})" upper="$(eval ${PI})" effort="25" velocity="6.0"/>
  </joint>

  <!-- END EFFECTOR -->
  <link name="end_effector">
    <visual>
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.02"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <origin xyz="0 0 0.02"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="end_effector_joint" type="fixed">
    <parent link="link6"/>
    <child link="end_effector"/>
    <origin xyz="0 0 0.06" rpy="0 0 0"/>
  </joint>

</robot>
